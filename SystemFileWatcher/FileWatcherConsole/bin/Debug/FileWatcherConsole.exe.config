<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <configSections>
        <section name="fileWatcher" type="FileWatcherConsole.ConfigParser.FileSystemWatcherConfigurationSection, FileWatcherConsole"/>
    </configSections>
    <startup>
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2"/>
    </startup>
    <fileWatcher culture="be-oy">
        <directories>
            <directory path="D:\Temp\Listen"/>
        </directories>
        <rules defaultDir="D:\Temp\appointment">
            <rule fileNamePattern="1.txt" appointmentFolder="D:\Temp\appointment" isOrderNumberAdd="true" isDateAdd="false"/>
            <rule fileNamePattern="hello1.rar*" appointmentFolder="D:\Temp\appointment" isOrderNumberAdd="false" isDateAdd="true"/>
        </rules>
    </fileWatcher>
</configuration>